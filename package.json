{
  "name": "test-ci-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apisauce": "^1.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "history": "^4.7.2",
    "i18next": "^11.9.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "postcss": "^7.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-spinkit": "^3.0.0",
    "redux": "^4.0.0",
    "redux-beacon": "^2.0.3",
    "redux-form": "^7.4.2",
    "redux-recompose": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "wolox-equalizer": "0.0.3"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version wolox-react-scripts",
    "build": "react-app-rewired build --scripts-version wolox-react-scripts",
    "test": "react-app-rewired test --scripts-version wolox-react-scripts --env=jsdom",
    "eject": "./node_modules/react-scripts/bin/react-scripts.js eject",
    "deploy": "react-app-rewired build --scripts-version wolox-react-scripts",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "lint-fix": "./node_modules/eslint/bin/eslint.js src --fix",
    "lint-diff": "git diff --name-only --cached --relative | grep \\.js$ | xargs eslint",
    "precommit": "npm run lint-diff",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/react": "^3.4.11",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^2.0.4",
    "wolox-react-scripts": "^1.2.1"
  },
  "title": "test-ci-frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolox/test-ci-frontend"
  }
}
